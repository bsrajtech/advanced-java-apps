<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>register-web-app</display-name>


	<servlet>
		<servlet-name>RegisterHttpServlet</servlet-name>
		<servlet-class>com.bsrajtech.sampleapp.servlets.RegisterHttpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterHttpServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginHttpServlet</servlet-name>
		<servlet-class>com.bsrajtech.sampleapp.servlets.LoginHttpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginHttpServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>  
  
</web-app>